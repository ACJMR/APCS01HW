public class Greet{
    public static void main(String[] args){
	greet("Colin");
	greet("Ducky");
       	greet("Dr. Hosking");
    }
    public static void greet(String name){
	System.out.println("Hello, "+  name + " , and welcome to the Aperture Science Computer-Aided Enrichment Center. ");
    }
    
}

